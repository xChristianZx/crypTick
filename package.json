{
  "name": "cryptick",
  "version": "1.0.0",
  "description": "A real-time cryptocurrency pricetracker",
  "main": "index.js",
  "engines": {
    "node": "9.5.0",
    "npm": "5.7.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
